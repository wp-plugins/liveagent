<div class="buttonTableRow">
	<div class='laButtonsTableCell'>
		<div class="laButtonPreview">
			<iframe id="{controls.la-htmlonlinebutton-id}"></iframe>			
		</div>
	</div>
	<div class='laButtonsTableCell'>
		<div class="laButtonPreview">
			<iframe id="{controls.la-htmlofflinebutton-id}"></iframe>
		</div>
	</div>
	<div class='laButtonsTableCell'>
		<div class='laButtonsTableCellType'>
		{controls.buttonType}
		</div>
	</div>
	<div class='laButtonsTableCell laButtonsTableEnabled'>
		{controls.isEnabled}
	</div>	
</div>
<!-- END: main -->