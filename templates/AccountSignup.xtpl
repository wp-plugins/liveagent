<!-- BEGIN: main -->
<table cellpadding="5px">
    <tr>
        <td><div class="logo"></div></td>
        <td>
            <h2>{controls.LiveAgentFreeHelpdeskAndLiveChat}</h2>
            <p>{controls.LiveAgentFreeHelpdeskAndLiveChatDescription}</p>
        </td>
    </tr>
</table>
<div class="wrap">
    <h2>{controls.CreateYourFreeAccount}</h2>
    {controls.errorMessages}    
    <form method="post" action="options.php">
        {controls.form-settings}    
        <table class="form-table">
            <tr>
                <th><label for="la-full-name">{controls.fullName}</label></th>
                <td>
                    {controls.la-full-name}
                    <span class="description">{controls.yourFirstNameAndLastName}</span>
                </td>            
            </tr>
            <tr>
                <th><label for="la-owner-email">{controls.Email}</label></th>
                <td>
                    {controls.la-owner-email}                
                </td>            
            </tr>
            <tr>
                <th><label for="la-url">{controls.domainSelection}</label></th>
                <td>
                    {controls.la-url}
                    <span class="description">{controls.ladeskCom}</span>                
                </td>            
            </tr>
        </table>
    </form>
    <br/>
    <p>{controls.iAgreeWith} <a href="http://www.qualityunit.com/liveagent/pricing/hosted/terms-and-conditions" target="_blank">{controls.TermsAndConditions}</a></p>
    <p class="submit"> 
        <input type="submit" value="{controls.submit}" class="button-primary" id="submit" name="submit">
    </p>
    <a href="{controls.skipCreateUrl}">{controls.SkipThisStepIHaveAccount}</a>    
<!-- END: main -->