<!-- BEGIN: main -->
<div class="wrap buttonsSettings">
	<div class="nlAboveStepBox">
		<!-- BEGIN: login_check_ok -->
			<div class="updated">
				{ok-icon}<div class="InfoText">{connection-ok}</div>
			</div>
		<!-- END: login_check_ok -->
       	<!-- BEGIN: login_check_failed -->
			<div class="error"><p>{connection-error}</p></div>
		<!-- END: login_check_failed -->
		<div class="updated">
           	{controls.bulb-icon}<div class="InfoText">You can customize Live chat buttons in admin panel by clicking on <b>"Login to Admin panel"</b> bellow.</div>
        </div>        
        <div class="nlStepBoxLeft">
            {controls.la-signup-button}
        </div>
        <div class="clear"></div>
	</div>	
    <form method="post" action="options.php">
    	{controls.form-settings}
    	<div class="nlStepBox">
		<div class="nlStepHeader">
			<div class="nlStepHeaderLeft">
				<span class="nlStepName">
					Integrate contact button with your website
				</span>
			</div>
			<div class="nlStepHeaderRight">
                <div class="nlLightText">
	                Do you need help with this plugin?
                </div>
                <div class="nlLightText">
                	Feel free to 
                    <a href="http://support.qualityunit.com/submit_ticket">
                    	contact us
                    </a>
                </div>
			</div>
			<div class="clear"></div>
		</div>
		<div class="nlStepBoxContent ButtonCodeCotent">
		<div class="nlFormFieldTextArea">
                        {controls.la-buttons_buttoncode}
                    </div>
		</div>
	 </div>
	 <div class="buttonsSave">
     <a class="nlBigButton" href="javascript:document.getElementsByName('la-config-buttons-page')[0].submit()">
      	{controls.submit}
     </a>
     </div>
    </form>
</div>
